{
  "expo": {
    "name": "Semi-Calories",
    "slug": "Semi-Calories",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/SplashLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/Logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/Logo.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Semi-Calories to access your camera."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Semi-Calories to access your photos.",
          "savePhotosPermission": "Allow Semi-Calories to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
