{
  "expo": {
    "name": "Semi-Calories",
    "slug": "Semi-Calories",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/SplashLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.shimhyesu.SemiCalories",
      "buildNumber": "1.0.7",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "유저 사진 설정, 식단 기록을 위해 카메라 권한이 필요합니다.",
        "NSPhotoLibraryUsageDescription": "유저 사진 설정, 식단 기록을 위해 앨범 권한이 필요합니다.",
        "NSPhotoLibraryAddUsageDescription": "Allow Semi-Calories to save photos."
      }
    },
    "android": {
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/Logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ],
      "package": "com.shimhyesu.SemiCalories"
    },
    "web": {
      "favicon": "./src/assets/Logo.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "유저 사진 설정, 식단 기록을 위해 카메라 권한이 필요합니다."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "유저 사진 설정, 식단 기록을 위해 앨범 권한이 필요합니다.",
          "savePhotosPermission": "Allows Semi-Calories to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "011405d4-54e5-458c-a39c-e40ce940e685"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/011405d4-54e5-458c-a39c-e40ce940e685"
    }
  }
}
